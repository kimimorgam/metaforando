        "\n",
        "import cv2\n",
        "import os\n",
        "from deepface import DeepFace\n",
        "import shutil\n",
        "from collections import Counter\n",
        "from google.colab import files\n",
        "import tensorflow as tf\n",
        "from tensorflow.python.client import device_lib\n",
        "\n",
        "# Upload file using Colab's file upload feature\n",
        "uploaded = files.upload()\n",
        "file_name = list(uploaded.keys())[0]\n",
        "\n",
        "# Check if GPU is available\n",
        "def check_available_gpus():\n",
        "    local_device_protos = device_lib.list_local_devices()\n",
        "    return [x.name for x in local_device_protos if x.device_type == 'GPU']\n",
        "\n",
        "available_gpus = check_available_gpus()\n",
        "print(f'Available GPUs: {available_gpus}')\n",
        "\n",
        "# If GPUs are available, set OpenCV to use the first one\n",
        "if available_gpus:\n",
        "    cv2.ocl.setUseOpenCL(True)\n",
        "    cv2.cuda.setDevice(0)\n",
        "else:\n",
        "    cv2.ocl.setUseOpenCL(False)\n",
        "\n",
        "# ... (rest of the code for video processing and DeepFace analysis)\n",
        "# The Tkinter dependent code for creating windows and buttons is removed.\n",
        "# Instead, the video processing function will be called directly with the uploaded file.\n",
        "\n",
        "from tqdm.notebook import tqdm\n",
        "\n",
        "def process_video(file_name):\n",
        "    # Open the video\n",
        "    cap = cv2.VideoCapture(file_name)\n",
        "\n",
        "    # Get the frames per second (FPS) of the video\n",
        "    fps = int(cap.get(cv2.CAP_PROP_FPS))\n",
        "\n",
        "    # Get the total number of frames in the video\n",
        "    total_frames = int(cap.get(cv2.CAP_PROP_FRAME_COUNT))\n",
        "\n",
        "    # Create a directory to save the extracted frames\n",
        "    if not os.path.exists('frames'):\n",
        "        os.makedirs('frames')\n",
        "\n",
        "    # Create a directory to save the analyzed frames\n",
        "    if not os.path.exists('analyzed_frames'):\n",
        "        os.makedirs('analyzed_frames')\n",
        "\n",
        "    # Initialize frame count and emotion list\n",
        "    frame_count = 0\n",
        "    emotions = []\n",
        "\n",
        "    # Initialize tqdm progress bar\n",
        "    with tqdm(total=total_frames//fps, desc='Processing frames') as pbar:\n",
        "        while cap.isOpened():\n",
        "            ret, frame = cap.read()\n",
        "            if ret == True:\n",
        "                if frame_count % fps == 0:  # Process one frame per second\n",
        "                    cv2.imwrite(f'frames/frame{frame_count}.jpg', frame)\n",
        "                    pbar.update(1)\n",
        "                frame_count += 1\n",
        "            else:\n",
        "                break\n",
        "\n",
        "    cap.release()\n",
        "    cv2.destroyAllWindows()\n",
        "\n",
        "    # ... (rest of the code remains the same as before)\n",
        "\n",
        "def process_video(file_name):\n",
        "    # Open the video\n",
        "    cap = cv2.VideoCapture(file_name)\n",
        "\n",
        "    # Get the frames per second (FPS) of the video\n",
        "    fps = int(cap.get(cv2.CAP_PROP_FPS))\n",
        "\n",
        "    # Create a directory to save the extracted frames\n",
        "    if not os.path.exists('frames'):\n",
        "        os.makedirs('frames')\n",
        "\n",
        "    # Create a directory to save the analyzed frames\n",
        "    if not os.path.exists('analyzed_frames'):\n",
        "        os.makedirs('analyzed_frames')\n",
        "\n",
        "    # Initialize frame count and emotion list\n",
        "    frame_count = 0\n",
        "    emotions = []\n",
        "\n",
        "    while cap.isOpened():\n",
        "        ret, frame = cap.read()\n",
        "        if ret == True:\n",
        "            if frame_count % fps == 0:  # Process one frame per second\n",
        "                cv2.imwrite(f'frames/frame{frame_count}.jpg', frame)\n",
        "\n",
        "            frame_count += 1\n",
        "        else:\n",
        "            break\n",
        "\n",
        "    cap.release()\n",
        "    cv2.destroyAllWindows()\n",
        "\n",
        "    # Analyze each extracted frame (1 frame per second)\n",
        "    for i in range(0, frame_count, fps):\n",
        "        img_path = f'frames/frame{i}.jpg'\n",
        "        img = cv2.imread(img_path)\n",
        "        results = DeepFace.analyze(img, actions=['age', 'gender', 'race', 'emotion'], enforce_detection=False)\n",
        "\n",
        "        for j, result in enumerate(results):\n",
        "            text = f\"Face {j+1}:\\nAge: {result['age']}\\nGender: {result['gender']}\\nRace: {result['dominant_race']}\\nEmotion: {result['dominant_emotion']}\"\n",
        "            emotions.append(result['dominant_emotion'])\n",
        "            img = draw_text_with_background_opencv(img, text, j)\n",
        "\n",
        "        cv2.imwrite(f'analyzed_frames/frame{i}.jpg', img)\n",
        "\n",
        "    # Compile the video back\n",
        "    img_array = []\n",
        "    for i in range(0, frame_count, fps):\n",
        "        img_path = f'analyzed_frames/frame{i}.jpg'\n",
        "        img = cv2.imread(img_path)\n",
        "        height, width, layers = img.shape\n",
        "        size = (width, height)\n",
        "        img_array.append(img)\n",
        "\n",
        "    out = cv2.VideoWriter('project.avi', cv2.VideoWriter_fourcc(*'DIVX'), fps, size)\n",
        "    for i in range(len(img_array)):\n",
        "        out.write(img_array[i])\n",
        "    out.release()\n",
        "\n",
        "    # Remove directories after use\n",
        "    shutil.rmtree('frames')\n",
        "    shutil.rmtree('analyzed_frames')\n",
        "\n",
        "    # Analyze emotions\n",
        "    emotion_counter = Counter(emotions)\n",
        "    total_emotions = sum(emotion_counter.values())\n",
        "    emotion_percentages = {emotion: count / total_emotions * 100 for emotion, count in emotion_counter.items()}\n",
        "    print(\"Emotion Analysis:\")\n",
        "    for emotion, percentage in emotion_percentages.items():\n",
        "        print(f\"Emotion: {emotion}, Percentage: {percentage:.2f}%\")\n",
        "\n",
        "# Call the function with the uploaded file name\n",
        "process_video(file_name)"
      ]
    }
  ]
}